  "devDependencies": {
    "bn.js": "5.0.0",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eth-gas-reporter": "0.2.0",
    "ethlint": "1.2.3",
